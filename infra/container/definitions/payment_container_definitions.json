[
  {
    "name": "mikes-pagamento-container",
    "image": "644237782704.dkr.ecr.us-east-2.amazonaws.com/mikes_payment:latest",
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8050,
        "hostPort": 8050
      }
    ],
    "environment": [
      {
        "name": "SQS_PAYMENT",
        "value": "${SQS_PAYMENT}"
      },
      {
        "name": "SNS_PAYMENT_STATUS",
        "value": "${SNS_PAYMENT_STATUS}"
      },
      {
        "name": "AWS_REGION",
        "value": "${REGION}"
      },
      {
        "name": "DB_HOST",
        "value": "${DB_HOST}"
      },
      {
        "name": "DB_PORT",
        "value": "${DB_PORT}"
      },
      {
        "name": "DB_NAME",
        "value": "${DB_NAME}"
      },
      {
        "name": "DB_USER",
        "value": "${DB_USER}"
      },
      {
        "name": "DB_PASSWORD",
        "value": "${DB_PASSWORD}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${LOG_GROUP_NAME}",
        "awslogs-region": "us-east-2",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]